{
  "name": "web-dependency-formatter",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:client": "react-scripts start",
    "start:server": "node ./server",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "test:ci": "npm run test -- --watchAll=false --env=node",
    "lint": "npm run lint:formatting && npm run lint:script",
    "lint:script": "eslint ./src/**/*.{ts,tsx}",
    "lint:styles": "stylelint ./src/**/*.{css,ts,tsx}",
    "lint:formatting": "prettier --check **/*",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/core": "~10.0",
    "@emotion/css": "~10.0",
    "@emotion/styled": "~10.0",
    "@reduxjs/toolkit": "~1.3",
    "@testing-library/jest-dom": "~5.5",
    "@testing-library/react": "~10.0",
    "@testing-library/user-event": "~10.0",
    "@types/jest": "~25.2",
    "@types/node": "~13.11",
    "@types/react": "~16.9",
    "@types/react-dom": "~16.9",
    "@types/react-redux": "~7.1",
    "@types/react-router-dom": "~5.1",
    "@types/recompose": "~0.30",
    "@types/semver": "~7.1",
    "@types/yup": "~0.28",
    "cors": "~2.8",
    "express": "~4.17",
    "formik": "~2.1",
    "node-fetch": "~2.6",
    "npm-registry-fetch": "~8.0",
    "npm-run-all": "~4.1",
    "react": "~16.13",
    "react-dom": "~16.13",
    "react-redux": "~7.2",
    "react-router-dom": "~5.1",
    "react-scripts": "~3.4",
    "recompose": "~0.30",
    "semver": "~7.1",
    "typescript": "~3.8",
    "yup": "~0.28"
  },
  "devDependencies": {
    "@commitlint/cli": "~8.3",
    "@commitlint/config-conventional": "~8.3",
    "@typescript-eslint/eslint-plugin": "~2.28",
    "@typescript-eslint/parser": "~2.28",
    "eslint": "~6.8",
    "eslint-config-airbnb-typescript": "~7.2",
    "eslint-config-prettier": "~6.10",
    "eslint-plugin-emotion": "~10.0",
    "eslint-plugin-import": "~2.20",
    "eslint-plugin-jsx-a11y": "~6.2",
    "eslint-plugin-prettier": "~3.1",
    "eslint-plugin-react": "~7.19",
    "eslint-plugin-react-hooks": "~3.0",
    "husky": "~4.2",
    "lint-staged": "~10.1",
    "prettier": "~2.0",
    "stylelint": "~13.3",
    "stylelint-config-idiomatic-order": "~8.1",
    "stylelint-config-standard": "~20.0",
    "stylelint-declaration-block-no-ignored-properties": "~2.3",
    "stylelint-high-performance-animation": "~1.5",
    "stylelint-prettier": "~1.1",
    "stylelint-use-logical": "~1.1",
    "stylelint-use-nesting": "~2.0"
  }
}
